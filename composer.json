{
  "require": {
    "craftcms/cms": "^3.7.26",
    "focuslabllc/craft-cheat-sheet": "^2.0",
    "hashtagerrors/syntax-highlighter": "1.0",
    "monachilada/craft-matrixtoolbar": "^1.0",
    "nystudio107/craft-imageoptimize": "^1.6",
    "verbb/expanded-singles": "^1.1",
    "verbb/navigation": "^1.4",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.14"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
